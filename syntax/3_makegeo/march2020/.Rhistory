#split into current  different administrative levels
#current
cadre_harmonise_caf_ipc_mar2020current <- cadre_harmonise_caf_ipc %>%
filter(exercise_year == 2020 & exercise_label == "Jan-May" & chtype == "current")
#areas analyzed at the adm0.5 level
cadre_harmonise_caf_ipc_mar2020_adm0_5 <- cadre_harmonise_caf_ipc_mar2020current %>%
filter(is.na(adm1_name) & is.na(adm1_5_name) & is.na(adm2_name) & is.na(adm2_5_name) & is.na(adm3_name))
#areas analyzed at the adm1 level
cadre_harmonise_caf_ipc_mar2020_adm1 <- cadre_harmonise_caf_ipc_mar2020current %>%
filter(is.na(adm1_5_name) & is.na(adm2_name) & is.na(adm2_5_name) & is.na(adm3_name))
#areas analyzed at the adm1.5 level
cadre_harmonise_caf_ipc_mar2020_adm1_5 <- cadre_harmonise_caf_ipc_mar2020current %>%
filter(!is.na(adm1_5_name) & is.na(adm2_name) & is.na(adm2_5_name) & is.na(adm3_name))
#areas analyzed at the adm2 level
cadre_harmonise_caf_ipc_mar2020_adm2 <- cadre_harmonise_caf_ipc_mar2020current %>%
filter(!is.na(adm2_name) & is.na(adm2_5_name) & is.na(adm3_name))
#areas analyzed at the adm2.5 level
cadre_harmonise_caf_ipc_mar2020_adm2_5 <- cadre_harmonise_caf_ipc_mar2020current %>%
filter(!is.na(adm2_5_name) & is.na(adm3_name))
#areas analyzed at the adm3 level
cadre_harmonise_caf_ipc_mar2020_adm3 <- cadre_harmonise_caf_ipc_mar2020current %>%
filter(!is.na(adm3_name))
